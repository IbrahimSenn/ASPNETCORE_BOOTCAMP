@model List<Bootcamp>;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>

@foreach(var kurs in Model)
{
    <div>
        <img src="~/img/@kurs.Image" width="300px" alt="">
        <h2> @kurs.Title </h2>
        <p> @kurs.Description </p>
    </div>
 
}
</body>
</html>